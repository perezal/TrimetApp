{
  "name": "trimet-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "moment": "^2.22.1",
    "node-sass-chokidar": "^1.2.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-scripts": "^1.1.4",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
